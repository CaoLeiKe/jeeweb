<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- 名称 -->
	<name>生成器配置</name>
	<!-- 描述 -->
	<description>生成配置</description>
	<generators>
		<generator>
			<!-- 注册关键字 -->
			<key>Controller</key>
			<!-- 文件层 -->
			<layer>controller</layer>
			<!-- 模版名称 -->
			<template-file>ControllerSimpleTemplate.ftl</template-file>
			<!-- 1.JAVA,2.JSP 3.MAPPER 4.Test代码 -->
			<code-type>1</code-type>
			<!-- 生成的文件名称，关键字[entityName] -->
			<name-format>[entityName]Controller.java</name-format>
		</generator>
		<generator>
			<!-- 注册关键字 -->
			<key>Service</key>
			<!-- 文件层 -->
			<layer>service</layer>
			<!-- 模版名称 -->
