			<key>testJava</key>
			<!-- 文件层级关系 -->
			<layer></layer>
			<!-- 模版名称 -->
			<template-file>javaTestTemplate.ftl</template-file>
			<!-- 1.JAVA,2.JSP 3.MAPPER 4.Test代码 -->
