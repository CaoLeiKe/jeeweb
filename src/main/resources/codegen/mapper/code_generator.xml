			<name-format>[entityName].java</name-format>
		</generator>
		<generator>
			<!-- 注册关键字 -->
			<key>viewList</key>
			<!-- 文件层级关系 -->
			<layer>[entityName]</layer>
			<!-- 模版名称 -->
			<template-file>viewListTemplate.ftl</template-file>
			<!-- 1.JAVA,2.JSP 3.MAPPER 4.Test代码 -->
			<code-type>2</code-type>
			<!-- 生成的文件名称，关键字[entityName] -->
			<name-format>list.jsp</name-format>
		</generator>
		<generator>
			<!-- 注册关键字 -->
			<key>viewForm</key>
			<!-- 文件层级关系 -->
			<layer>[entityName]</layer>
			<!-- 模版名称 -->
			<template-file>viewFormTemplate.ftl</template-file>
			<!-- 1.JAVA,2.JSP 3.MAPPER 4.Test代码 -->
